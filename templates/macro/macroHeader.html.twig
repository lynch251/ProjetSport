{% macro headerUser(test ="") %}
<!-- Header -->
<header class="masthead">
    <div class="container">
        <div class="intro-text">
            {%  if is_granted('ROLE_ADMIN') %}
                <div class="intro-lead-in"> Bienvenue {{ app.user.prenomUtilisateur }} !</br><img class="mt-3" src="{{ app.user.logo }}" width="80px" alt="profile"/></div>
                Dash Admin
            {%  elseif is_granted('ROLE_USER') %}
                <div class="intro-lead-in"> Bienvenue {{ app.user.prenomUtilisateur }} !</br><img class="mt-3" src="{{ app.user.logo }}" width="80px" alt="profile"/></div>
                <a class="btn btn-primary btn-xl text-uppercase" href="/seance/new">Débuter une séance</a>
            {% else %}
                <div class="intro-lead-in"> Bienvenue !</div>
            {% endif %}

        </div>
    </div>
</header>
{% endmacro%}